CREATE TABLE MAIN_TRANSACTION_FD (
SERIAL_NO SERIAL  , 
ACCNO INT NOT NULL PRIMARY KEY UNIQUE , 
CLIENT_NAME VARCHAR NOT NULL , 
MOBLIE_NO VARCHAR ,
BALANCE INT,
RATE_OF_INTEREST VARCHAR UNIQUE , 
TIME_SPAM(YEARS) VARCHAR,
CONSTRAINT fk_customer
      FOREIGN KEY(ACCNO)  
	  REFERENCES CUSTINFO(ACCNO));
